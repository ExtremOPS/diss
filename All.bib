journal = {Journal of Micromechanics and Microengineering},
number = {3},
pages = {034004},
title = {{Rotational speed measurements of small spherical particles driven by acoustic viscous torques utilizing an optical trap}},
url = {http://iopscience.iop.org/article/10.1088/1361-6439/abde92},
volume = {31},
year = {2021}
}
